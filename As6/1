#include <stdio.h>
#include <unistd.h>
#include <signal.h>
#include <string.h>
#include <stdlib.h>
#include <ctype.h>

void sig2str(int signo, char *str){


char *test = strdup(sys_siglist[signo]);
strcpy(str, test);

}


int main()
{

char *test = malloc(1000);
sig2str(17,test);

printf("%s\n",test);
}
